{
  "name": "ts-nextjs-sample",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "commit": "git-cz"
  },
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "@wry/equality": "^0.1.11",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "axios": "^0.19.2",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.26",
    "dotenv-webpack": "^1.8.0",
    "lodash": "^4.17.19",
    "loglevel": "^1.6.8",
    "next": "9.4.0",
    "next-images": "^1.4.0",
    "next-pwa": "^2.4.1",
    "polished": "^3.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.5",
    "styled-components": "^5.1.0",
    "swr": "^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/lodash": "^4.14.150",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-redux": "^7.1.8",
    "@types/redux-actions": "^2.6.1",
    "@types/styled-components": "^5.1.0",
    "@umijs/fabric": "^2.0.8",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.2"
  }
}
